<ol id="message_list" class="message_list" data-controller="message-list">
  <turbo-frame id="message_list_<%= room.id %>" >
    <%= turbo_stream_from "messages_#{room.id}" %>
    <%= render partial: "messages/message", collection: room.messages.includes(:user).order(:created_at) %>
  </turbo-frame>
</ol>
